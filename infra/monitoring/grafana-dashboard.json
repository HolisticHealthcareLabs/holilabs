{
  "dashboard": {
    "title": "Holi Labs - FHIR Integration Monitoring",
    "uid": "holi-fhir-monitoring",
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "tags": ["holi", "fhir", "medplum", "hipaa"],
    "panels": [
      {
        "id": 1,
        "title": "System Health Overview",
        "type": "stat",
        "gridPos": { "x": 0, "y": 0, "w": 24, "h": 4 },
        "targets": [
          {
            "expr": "up{job=\"holi-api\"}",
            "legendFormat": "API Server",
            "refId": "A"
          },
          {
            "expr": "holi_database_connections_active > 0",
            "legendFormat": "Database",
            "refId": "B"
          },
          {
            "expr": "holi_queue_jobs_active{queue_name=\"fhir-sync\"} >= 0",
            "legendFormat": "Queue",
            "refId": "C"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              { "options": { "0": { "text": "Down", "color": "red" } } },
              { "options": { "1": { "text": "Up", "color": "green" } } }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 1, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "HTTP Request Rate (requests/min)",
        "type": "graph",
        "gridPos": { "x": 0, "y": 4, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(holi_http_requests_total[5m])) by (method) * 60",
            "legendFormat": "{{method}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "req/min",
            "format": "short"
          }
        ]
      },
      {
        "id": 3,
        "title": "HTTP Response Time (p95)",
        "type": "graph",
        "gridPos": { "x": 12, "y": 4, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(holi_http_request_duration_seconds_bucket[5m])) by (le, route))",
            "legendFormat": "{{route}} (p95)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(holi_http_request_duration_seconds_bucket[5m])) by (le, route))",
            "legendFormat": "{{route}} (p50)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "label": "seconds",
            "format": "s"
          }
        ]
      },
      {
        "id": 4,
        "title": "Queue Health - Active Jobs",
        "type": "graph",
        "gridPos": { "x": 0, "y": 12, "w": 8, "h": 8 },
        "targets": [
          {
            "expr": "holi_queue_jobs_active{queue_name=\"fhir-sync\"}",
            "legendFormat": "Active",
            "refId": "A"
          },
          {
            "expr": "holi_queue_jobs_waiting{queue_name=\"fhir-sync\"}",
            "legendFormat": "Waiting",
            "refId": "B"
          }
        ],
        "alert": {
          "name": "High Queue Backlog",
          "conditions": [
            {
              "evaluator": {
                "params": [100],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["B", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "1m",
          "message": "FHIR sync queue has >100 waiting jobs for 5+ minutes",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 5,
        "title": "Queue Health - Failed Jobs",
        "type": "graph",
        "gridPos": { "x": 8, "y": 12, "w": 8, "h": 8 },
        "targets": [
          {
            "expr": "holi_queue_jobs_failed{queue_name=\"fhir-sync\"}",
            "legendFormat": "Failed",
            "refId": "A"
          },
          {
            "expr": "rate(holi_queue_job_errors_total{queue_name=\"fhir-sync\"}[5m])",
            "legendFormat": "Error Rate",
            "refId": "B"
          }
        ],
        "alert": {
          "name": "Excessive Queue Failures",
          "conditions": [
            {
              "evaluator": {
                "params": [10],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "1m",
          "message": "FHIR sync queue has >10 failed jobs",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 6,
        "title": "Queue Job Duration (p95)",
        "type": "graph",
        "gridPos": { "x": 16, "y": 12, "w": 8, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(holi_queue_job_duration_seconds_bucket{queue_name=\"fhir-sync\"}[5m])) by (le, job_type))",
            "legendFormat": "{{job_type}} (p95)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "seconds",
            "format": "s"
          }
        ]
      },
      {
        "id": 7,
        "title": "FHIR Sync - Not Synced Resources",
        "type": "stat",
        "gridPos": { "x": 0, "y": 20, "w": 8, "h": 4 },
        "targets": [
          {
            "expr": "holi_fhir_sync_not_synced{resource_type=\"Encounter\"}",
            "legendFormat": "Encounters",
            "refId": "A"
          },
          {
            "expr": "holi_fhir_sync_not_synced{resource_type=\"Observation\"}",
            "legendFormat": "Observations",
            "refId": "B"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 10, "color": "yellow" },
                { "value": 50, "color": "red" }
              ]
            }
          }
        },
        "alert": {
          "name": "Unsynced Resources",
          "conditions": [
            {
              "evaluator": {
                "params": [100],
                "type": "gt"
              },
              "operator": {
                "type": "or"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "10m",
          "frequency": "1m",
          "message": "More than 100 resources have never been synced to FHIR",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 8,
        "title": "FHIR Sync - Stale Resources (>1h)",
        "type": "stat",
        "gridPos": { "x": 8, "y": 20, "w": 8, "h": 4 },
        "targets": [
          {
            "expr": "holi_fhir_sync_stale{resource_type=\"Encounter\"}",
            "legendFormat": "Encounters",
            "refId": "A"
          },
          {
            "expr": "holi_fhir_sync_stale{resource_type=\"Observation\"}",
            "legendFormat": "Observations",
            "refId": "B"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 5, "color": "yellow" },
                { "value": 20, "color": "red" }
              ]
            }
          }
        },
        "alert": {
          "name": "Stale FHIR Resources",
          "conditions": [
            {
              "evaluator": {
                "params": [50],
                "type": "gt"
              },
              "operator": {
                "type": "or"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "15m",
          "frequency": "1m",
          "message": "More than 50 resources have stale FHIR sync (>1 hour)",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 9,
        "title": "FHIR Sync Operations Rate",
        "type": "graph",
        "gridPos": { "x": 16, "y": 20, "w": 8, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(holi_fhir_sync_operations_total{status=\"success\"}[5m])) by (resource_type, operation) * 60",
            "legendFormat": "{{resource_type}}/{{operation}} (success)",
            "refId": "A"
          },
          {
            "expr": "sum(rate(holi_fhir_sync_operations_total{status=\"failure\"}[5m])) by (resource_type, operation) * 60",
            "legendFormat": "{{resource_type}}/{{operation}} (failure)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "label": "ops/min",
            "format": "short"
          }
        ]
      },
      {
        "id": 10,
        "title": "FHIR Sync Duration (p95)",
        "type": "graph",
        "gridPos": { "x": 0, "y": 24, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(holi_fhir_sync_duration_seconds_bucket[5m])) by (le, resource_type, operation))",
            "legendFormat": "{{resource_type}}/{{operation}} (p95)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "seconds",
            "format": "s"
          }
        ]
      },
      {
        "id": 11,
        "title": "FHIR Sync Error Rate",
        "type": "graph",
        "gridPos": { "x": 12, "y": 24, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(holi_fhir_sync_errors_total[5m])) by (resource_type, error_type) * 60",
            "legendFormat": "{{resource_type}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "errors/min",
            "format": "short"
          }
        ]
      },
      {
        "id": 12,
        "title": "Reconciliation Runs",
        "type": "graph",
        "gridPos": { "x": 0, "y": 32, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(holi_fhir_reconciliation_runs_total[1h])",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 13,
        "title": "Reconciliation Duration",
        "type": "graph",
        "gridPos": { "x": 12, "y": 32, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(holi_fhir_reconciliation_duration_seconds_bucket[1h])) by (le))",
            "legendFormat": "p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(holi_fhir_reconciliation_duration_seconds_bucket[1h])) by (le))",
            "legendFormat": "p50",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "label": "seconds",
            "format": "s"
          }
        ]
      },
      {
        "id": 14,
        "title": "Audit Mirror - Events Processed",
        "type": "graph",
        "gridPos": { "x": 0, "y": 40, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(holi_fhir_audit_mirror_events_total{status=\"mirrored\"}[5m]) * 60",
            "legendFormat": "Mirrored",
            "refId": "A"
          },
          {
            "expr": "rate(holi_fhir_audit_mirror_events_total{status=\"skipped\"}[5m]) * 60",
            "legendFormat": "Skipped",
            "refId": "B"
          },
          {
            "expr": "rate(holi_fhir_audit_mirror_events_total{status=\"error\"}[5m]) * 60",
            "legendFormat": "Errors",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "label": "events/min",
            "format": "short"
          }
        ]
      },
      {
        "id": 15,
        "title": "Audit Mirror Duration",
        "type": "graph",
        "gridPos": { "x": 12, "y": 40, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(holi_fhir_audit_mirror_duration_seconds_bucket[1h])) by (le))",
            "legendFormat": "p95",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "seconds",
            "format": "s"
          }
        ]
      },
      {
        "id": 16,
        "title": "HIPAA Audit Events (by type)",
        "type": "graph",
        "gridPos": { "x": 0, "y": 48, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(holi_hipaa_audit_events_total[5m])) by (event_type) * 60",
            "legendFormat": "{{event_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "events/min",
            "format": "short"
          }
        ]
      },
      {
        "id": 17,
        "title": "PHI Access Events (by role)",
        "type": "graph",
        "gridPos": { "x": 12, "y": 48, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(holi_hipaa_phi_access_total[5m])) by (user_role, access_type) * 60",
            "legendFormat": "{{user_role}} - {{access_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "accesses/min",
            "format": "short"
          }
        ]
      },
      {
        "id": 18,
        "title": "Consent Validations",
        "type": "graph",
        "gridPos": { "x": 0, "y": 56, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(holi_hipaa_consent_validations_total[5m])) by (status) * 60",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "validations/min",
            "format": "short"
          }
        ]
      },
      {
        "id": 19,
        "title": "Database Connections",
        "type": "graph",
        "gridPos": { "x": 12, "y": 56, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "holi_database_connections_active",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ]
      },
      {
        "id": 20,
        "title": "Database Query Duration (p95)",
        "type": "graph",
        "gridPos": { "x": 0, "y": 64, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(holi_database_query_duration_seconds_bucket[5m])) by (le, model, operation))",
            "legendFormat": "{{model}}.{{operation}} (p95)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "seconds",
            "format": "s"
          }
        ]
      },
      {
        "id": 21,
        "title": "FHIR Operations (by type)",
        "type": "graph",
        "gridPos": { "x": 12, "y": 64, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(holi_fhir_operations_total{status=\"success\"}[5m])) by (operation, resource_type) * 60",
            "legendFormat": "{{operation}}/{{resource_type}} (success)",
            "refId": "A"
          },
          {
            "expr": "sum(rate(holi_fhir_operations_total{status=\"failure\"}[5m])) by (operation, resource_type) * 60",
            "legendFormat": "{{operation}}/{{resource_type}} (failure)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "label": "ops/min",
            "format": "short"
          }
        ]
      }
    ]
  },
  "overwrite": true
}
